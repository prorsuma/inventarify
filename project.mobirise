        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "@media (min-width: 1400px)": {
              ".mbr-col-lg-4, .mbr-col-lg-3": {
                "padding": "0.4rem"
              }
            },
            ".card-head": {
              "padding-bottom": "3rem",
              "padding-top": "4rem",
              ".amp-iconfont": {
                "color": "@iconColor !important",
                "font-size": "(@iconsSize * 1rem)",
                "width": "(@iconsSize * 1rem)",
                "margin-bottom": "1.5rem"
              }
            },
            ".iconfont-wrapper": {
              "height": "(@iconsSize * 1rem)",
              "width": "(@iconsSize * 1rem)"
            },
            ".card-price-wrap": {
              "padding": "1.5rem 2rem",
              "background-color": "#f6f6f6 !important"
            },
            ".price-sign": {
              "vertical-align": "super"
            },
            ".price-caption": {
              "vertical-align": "bottom"
            },
            ".btn": {
              "border-radius": "30px"
            },
            ".btn-black": {
              "background": "#6b6b6b !important",
              "border-color": "#6b6b6b !important",
              "&:hover": {
                "background": "@secondaryColor !important",
                "border-color": "@secondaryColor !important"
              }
            },
            ".items-list-wrap .list-item": {
              "& when (@strippedBg) and not (@transparentBg)": {
                "&:nth-child(odd)": {
                  "background-color": "@strippedBgColor"
                }
              },
              "& when (@showIcons)": {
                "display": "flex",
                "align-items": "center"
              }
            },
            "& when (@showIcons)": {
              ".list-item": {
                ".amp-iconfont": {
                  "width": "(@iconsSize2 * 1rem)",
                  "font-size": "(@iconsSize2 * 1rem)",
                  "color": "@iconColor2 !important",
                  "margin-right": "1rem"
                }
              }
            },
            "& when not (@transparentBg)": {
              "& when not (@checkDifferentBg)": {
                ".card-inner, .card-head, .card-price-wrap, .list-item": {
                  "background-color": "@cardBg"
                }
              },
              "& when (@checkDifferentBg)": {
                ".card1": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card1Bg"
                  }
                },
                ".card2": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card2Bg"
                  }
                },
                ".card3": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card3Bg"
                  }
                },
                ".card4": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card4Bg"
                  }
                }
              }
            },
            "& when (@transparentBg)": {
              ".card-inner, .card-price-wrap, .list-item": {
                "background": "transparent"
              },
              ".card-inner": {
                "position": "relative",
                ".mbr-overlay": {
                  "opacity": "@bgOpacity",
                  "background-color": "@cardBg"
                },
                "div:not(.mbr-overlay)": {
                  "z-index": "1"
                }
              }
            },
            "& when (@cardBorder)": {
              ".list-item": {
                "border-bottom": "1px solid @colorBorder"
              }
            },
            ".cardhead1": {
              "background-color": "@cardheadBg1 !important"
            },
            ".cardhead2": {
              "background-color": "@cardheadBg2 !important"
            },
            ".cardhead3": {
              "background-color": "@cardheadBg3 !important"
            },
            ".cardhead4": {
              "background-color": "@cardheadBg4 !important"
            },
            ".card-title": {
              "color": "#af938f"
            },
            "@media (max-width: 767px)": {
              ".card-head, .card-price-wrap, .list-item": {
                "padding-left": "1rem",
                "padding-right": "1rem"
              }
            },
            ".card1 .card-title": {
              "color": "#d8d8d8"
            },
            ".card2 .card-title": {
              "color": "#d8d8d8"
            },
            ".card3 .card-title": {
              "color": "#d8d8d8"
            }
          },
          "_name": "pricing-table01",
          "_customHTML": "<section class=\"pricing-table1\" group=\"Pricing Table\" plugins=\"amp-main\">\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n\t\t<!--  -->\n\t\t<header>Title</header>\n\t\t<input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n\t\t<input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n\t\t<select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n\t\t\t<option value=\"1\">Left</option>\n\t\t\t<option value=\"2\" selected>Center</option>\n\t\t\t<option value=\"3\">Right</option>\n\t\t</select>\n\t\t<header>Cards</header>\n\t\t<input type=\"range\" inline title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\">\n\t\t<select title=\"Align\" name=\"cardAlign\">\n\t\t\t<option value=\"1\">Left</option>\n\t\t\t<option value=\"2\" selected>Center</option>\n\t\t\t<option value=\"3\">Right</option>\n\t\t</select>\n\t\t<input type=\"range\" inline title=\"Items in 1\" name=\"card1Items\" min=\"1\" max=\"6\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Items in 2\" name=\"card2Items\" min=\"1\" max=\"6\" step=\"1\" value=\"4\" condition=\"cardsAmount > 1\">\n\t\t<input type=\"range\" inline title=\"Items in 3\" name=\"card3Items\" min=\"1\" max=\"6\" step=\"1\" value=\"4\" condition=\"cardsAmount > 2\">\n\t\t<input type=\"range\" inline title=\"Items in 4\" name=\"card4Items\" min=\"1\" max=\"6\" step=\"1\" value=\"4\" condition=\"cardsAmount > 3\">\n\t\t<input type=\"checkbox\" name=\"cardBorder\" title=\"Border\" checked>\n\t\t<input type=\"color\" name=\"colorBorder\" title=\"Border Color\" value=\"#f3f3f3\">\n\t\t<input type=\"color\" title=\"Card 1 Head BG\" name=\"cardheadBg1\" value=\"#2b2b2b\">\n\t\t<input type=\"color\" title=\"Card 2 Head BG\" name=\"cardheadBg2\" value=\"#2b2b2b\" condition=\"cardsAmount > 1\">\n\t\t<input type=\"color\" title=\"Card 3 Head BG\" name=\"cardheadBg3\" value=\"#2b2b2b\" condition=\"cardsAmount > 2\">\n\t\t<input type=\"color\" title=\"Card 4 Head BG\" name=\"cardheadBg4\" value=\"#2b2b2b\" condition=\"cardsAmount > 3\">\n\n\t\t<input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\">\n\t\t<input type=\"range\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" name=\"bgOpacity\" title=\"Opacity\" condition=\"transparentBg\">\n\t\t<input type=\"checkbox\" title=\"Different Card BG\" name=\"checkDifferentBg\" condition=\"!transparentBg\">\n\t\t<input type=\"color\" title=\"Card BG\" name=\"cardBg\" value=\"#ffffff\" condition=\"!checkDifferentBg || transparentBg\">\n\t\t<input type=\"color\" title=\"Card 1 BG\" name=\"card1Bg\" value=\"#81cbd2\" condition=\"checkDifferentBg && !transparentBg\">\n\t\t<input type=\"color\" title=\"Card 2 BG\" name=\"card2Bg\" value=\"#81cbd2\" condition=\"checkDifferentBg &amp;&amp; cardsAmount > 1 &amp;&amp; !transparentBg\">\n\t\t<input type=\"color\" title=\"Card 3 BG\" name=\"card3Bg\" value=\"#81cbd2\" condition=\"checkDifferentBg &amp;&amp; cardsAmount > 2 &amp;&amp; !transparentBg\">\n\t\t<input type=\"color\" title=\"Card 4 BG\" name=\"card4Bg\" value=\"#81cbd2\" condition=\"checkDifferentBg &amp;&amp; cardsAmount > 3 &amp;&amp; !transparentBg\">\n\t\t<input type=\"checkbox\" title=\"Stripped List BG\" name=\"strippedBg\" condition=\"!transparentBg\">\n\t\t<input type=\"color\" title=\"Color\" name=\"strippedBgColor\" value=\"#d26f6f\" condition=\"strippedBg && !transparentBg\">\n\t\t<input type=\"checkbox\" title=\"Price Text\" name=\"showPriceText\" checked>\n\t\t<input type=\"checkbox\" title=\"Icons\" name=\"showIcons\" checked>\n\t\t<input type=\"color\" t